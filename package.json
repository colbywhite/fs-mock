{
  "name": "fs-mock",
  "version": "1.0.0-alpha.1",
  "description": "A lib to help mock the file system during vitest tests",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc -p tsconfig.build.json",
    "format:fix": "prettier -w .",
    "format:check": "prettier -c .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "check": "run-p lint:check format:check",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "keywords": [
    "fs",
    "mock",
    "test",
    "testing",
    "vitest"
  ],
  "author": "Colby M. White",
  "license": "ISC",
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {}
}
